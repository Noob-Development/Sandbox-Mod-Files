<!-- THIS IS A COMMENT -->
<wargamepatch>
	<!-- Increases FOB supply from 16000 to 999999. -->
	<ndfpatch ndf="pc\ndf\patchable\gfx\everything.ndfbin" table="TModuleModernWarfareSupplyDescriptor" name="infiniteFob">
		<matchconditions>
			<!-- Searches for all supply instances that have a capacity of 16000, of which FOBs are the only one. -->
			<matchcondition type="property" property="SupplyCapacity">16000</matchcondition>
		</matchconditions>
		<changes>
			<!--Sets to 999999 supply points. -->
			<change operation="set" property="SupplyCapacity" type="Int32">999999</change>
		</changes>
	</ndfpatch>
</wargamepatch>