<!-- THIS IS A COMMENT -->
<wargamepatch>
	<ndfpatch ndf="pc\ndf\patchable\gfx\everything.ndfbin" table="TModernWarfareSupplyDescriptor" name="normalSupply">
		<matchconditions>
			<!-- This change will apply for all units, including trucks, FOBs, and supply helicopters. -->
			<matchcondition type="property" property="__order">first</matchcondition>
		</matchconditions>
		<changes>
			<change operation="set" property="FuelSupplyBySecond">30000</change>
			<change operation="set" property="HealthSupplyBySecond">100</change>
			<change operation="set" property="AmmunitionSupplyBySecond">25000</change>
			<change operation="set" property="SupplySupplyBySecond">25000</change>
			<!-- upplySupply = how fast it fills other supply vehicles lower on the "supply structure". The original values were 30, 0.1, 25, 25 respectively. -->
		</changes>
	</ndfpatch>
</wargamepatch>